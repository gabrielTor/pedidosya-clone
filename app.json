{
  "expo": {
    "name": "pedidosya-clone",
    "slug": "pedidosya-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.gif",
      "resizeMode": "cover",
      "backgroundColor": "rgb(250, 0, 80)"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "rgb(250, 0, 80)"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
